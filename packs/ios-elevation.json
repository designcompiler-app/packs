{
  "meta": {
    "id": "ios-elevation",
    "name": "iOS Elevation",
    "description": "Shadow tokens inspired by Apple HIG elevation â€” navigation bars, cards, sheets, modals, and alerts. Tuned for the subtle, diffused shadow style used across iOS and macOS.",
    "source": "Design Compiler",
    "sourceUrl": "https://stylefuse.io",
    "version": "1.0",
    "license": "CC0",
    "licenseUrl": "https://creativecommons.org/publicdomain/zero/1.0/",
    "tokenCount": 8,
    "categories": [
      "shadow"
    ],
    "preview": []
  },
  "data": {
    "shadow": {
      "$type": "shadow",
      "$description": "Apple-style diffused elevation shadows",
      "navigationBar": {
        "$value": {
          "offsetX": "0px",
          "offsetY": "0.5px",
          "blur": "0px",
          "spread": "0px",
          "color": "#0000004d"
        },
        "$description": "Navigation bar hairline shadow"
      },
      "card": {
        "$value": {
          "offsetX": "0px",
          "offsetY": "2px",
          "blur": "8px",
          "spread": "0px",
          "color": "#00000014"
        },
        "$description": "Card and grouped table section"
      },
      "tabBar": {
        "$value": {
          "offsetX": "0px",
          "offsetY": "-0.5px",
          "blur": "0px",
          "spread": "0px",
          "color": "#0000004d"
        },
        "$description": "Tab bar top edge shadow"
      },
      "sheet": {
        "$value": {
          "offsetX": "0px",
          "offsetY": "-3px",
          "blur": "12px",
          "spread": "0px",
          "color": "#0000001a"
        },
        "$description": "Bottom sheet or action sheet"
      },
      "popover": {
        "$value": {
          "offsetX": "0px",
          "offsetY": "4px",
          "blur": "20px",
          "spread": "0px",
          "color": "#00000026"
        },
        "$description": "Popover and context menu"
      },
      "modal": {
        "$value": {
          "offsetX": "0px",
          "offsetY": "8px",
          "blur": "32px",
          "spread": "0px",
          "color": "#00000033"
        },
        "$description": "Modal presentation and alert"
      },
      "tooltip": {
        "$value": {
          "offsetX": "0px",
          "offsetY": "2px",
          "blur": "10px",
          "spread": "0px",
          "color": "#0000001a"
        },
        "$description": "Tooltip and small overlay"
      },
      "drag": {
        "$value": {
          "offsetX": "0px",
          "offsetY": "12px",
          "blur": "40px",
          "spread": "-4px",
          "color": "#0000003d"
        },
        "$description": "Dragged element elevation"
      }
    }
  }
}
